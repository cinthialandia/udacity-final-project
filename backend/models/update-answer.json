{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "year": {
      "type": "number"
    },
    "answer": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string",
          "minLength": 1,
          "pattern": "^(?!s*$).+"
        }
      },
      "required": ["answer"]
    }
  },
  "required": ["year", "answer"],
  "additionalProperties": false
}
